{
    "name": "spiral/app",
    "type": "project",
    "license": "MIT",
    "description": "Spiral Skeleton Application",
    "homepage": "https://spiral.dev",
    "support": {
        "issues": "https://github.com/spiral/app/issues",
        "source": "https://github.com/spiral/app"
    },
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*",
        "cycle/database": "^2.13.0",
        "cycle/migrations": "^4.2.5",
        "cycle/orm": "~2.9.3",
        "cycle/schema-builder": "^2.11.1",
        "doctrine/collections": "^1.8",
        "gam6itko/cycle-nyholm-dsn-bridge": "^0.1.0",
        "league/flysystem-aws-s3-v3": "^3.28",
        "open-telemetry/exporter-otlp": "^1.0",
        "spiral-packages/league-event": "^1.0",
        "spiral/cycle-bridge": "^2.5",
        "spiral/framework": "^3.9",
        "spiral/nyholm-bridge": "^1.3",
        "spiral/otel-bridge": "^1.2",
        "spiral/profiler": "^3.2",
        "spiral/roadrunner-bridge": "^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3.6"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/src"
        },
        "files": [
            "app/src/Application/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "php-http/discovery": true,
            "spiral/composer-publish-plugin": true,
            "tbachert/spi": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
