{
    "name": "spiral/app-consumer",
    "type": "project",
    "license": "MIT",
    "require": {
        "php": ">=8.2",
        "ext-mbstring": "*",
        "doctrine/collections": "^1.8",
        "gam6itko/cycle-nyholm-dsn-bridge": "^0.1.0",
        "open-telemetry/exporter-otlp": "^1.0",
        "spiral/cycle-bridge": "^2.5",
        "spiral/framework": "^3.9",
        "spiral/nyholm-bridge": "^1.3",
        "spiral/otel-bridge": "^1.2",
        "spiral/profiler": "^3.2",
        "spiral/roadrunner-bridge": "^3.0"
    },
    "require-dev": {
        "symfony/var-dumper": "^6.3.6"
    },
    "autoload": {
        "psr-4": {
            "AppConsumer\\": "app/src"
        },
        "files": [
            "app/src/Application/helpers.php"
        ]
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "spiral/composer-publish-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
